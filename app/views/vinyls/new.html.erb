

<%= form_for @vinyl do |f| %>

  <%= f.label        :artist %>
  <%= f.text_field   :artist %>
  <%= f.label        :album %>
  <%= f.text_field   :album %>
  <%= f.label        :year %>
  <%= f.text_field   :year %>
  <%= f.label        :genre %>
  <%= f.text_field   :genre %>

  <%= f.submit "Add", class: "button" %>


<% end %>
